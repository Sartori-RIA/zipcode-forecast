<div class="center">
  <%= render partial: 'weather_forecasts/form', locals: { zipcode: @zipcode } %>

  <br>
  <br>
  <br>

  <% if @forecast.present? && !%w[404 400].include?(@geo["cod"]) %>
    <%= render partial: 'weather_forecasts/forecast', locals: { forecast: @forecast, geo: @geo, came_from_cache: @came_from_cache } %>
  <% end %>
</div>
